<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/pres_dialog_shape"
    android:id="@+id/predialog2" >


    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:background="@drawable/pres_dialog_title" >

        <ImageButton
            android:id="@+id/dialog_close1"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@drawable/img_dialogdelete"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="20dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="血压分析结果"
            android:gravity="center"
            android:textColor="#ffffff"
            android:textSize="20sp" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/dialogface"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_below="@+id/relativeLayout1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignLeft="@+id/dialog_button"
        android:layout_below="@+id/dialog_button"
        android:text=" " />

    <Button
        android:id="@+id/dialog_remind"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dialog_text"
        android:layout_marginTop="26dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/pres_dialog_button"
        android:padding="5dp"
        android:text="好，稍候提醒我"
        android:textColor="#ffffff"
        android:textSize="18sp" />

    <Button
        android:id="@+id/dialog_close2"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/dialog_remind"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@+id/dialog_remind"
        android:background="@drawable/pres_dialog_button"
        android:padding="5dp"
        android:text="不，不用了谢谢"
        android:textColor="#ffffff"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="10dp"
        android:layout_below="@+id/dialog_remind"
        android:layout_toRightOf="@+id/textView1"
        android:text=" " />

    <TextView
        android:id="@+id/dialog_text"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dialogface"
        android:layout_centerHorizontal="true"
        android:gravity="left"
        android:maxLines="5"
        android:text=" "
        android:textSize="18sp" />

</RelativeLayout>
