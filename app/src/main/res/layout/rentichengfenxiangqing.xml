<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/relativeLayout4"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/colorPrimary"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <RelativeLayout
            android:id="@+id/back_relative"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_centerVertical="true"
            android:weightSum="1" >

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="2dp"
                android:src="@drawable/jt_fh" />
        </RelativeLayout>

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="人体成分详情"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#fff"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:id="@+id/state_content_relative_measure_result"
        android:layout_below="@+id/relativeLayout4"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="本次测量结果"
            android:textSize="20sp"
            android:id="@+id/state_content_text_measure_result"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="8dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="体重："
            android:id="@+id/state_content_text_gaoya"
            android:layout_below="@+id/state_content_text_measure_result"
            android:layout_alignLeft="@+id/state_content_text_measure_result"
            android:layout_alignStart="@+id/state_content_text_measure_result" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="脂肪："
            android:id="@+id/state_content_text_diya"
            android:layout_below="@+id/state_content_text_gaoya"
            android:layout_alignLeft="@+id/state_content_text_gaoya"
            android:layout_alignStart="@+id/state_content_text_gaoya" />

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="骨量："
            android:id="@+id/state_content_text_xinlv"
            android:layout_below="@+id/state_content_text_diya"
            android:layout_alignLeft="@+id/state_content_text_diya"
            android:layout_alignStart="@+id/state_content_text_diya" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_taye4"
            android:layout_below="@+id/state_content_text_xinlv"
            android:layout_alignLeft="@+id/state_content_text_xinlv"
            android:layout_alignStart="@+id/state_content_text_xinlv"
            android:text="肌肉："
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_type5"
            android:layout_below="@+id/result_taye4"
            android:layout_alignLeft="@+id/result_taye4"
            android:layout_alignStart="@+id/result_taye4"
            android:text="内脏脂肪："/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_type6"
            android:layout_below="@+id/result_type5"
            android:layout_alignLeft="@+id/result_type5"
            android:layout_alignStart="@+id/result_type5"
            android:text="人体阻抗："/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_type7"
            android:layout_below="@+id/result_type6"
            android:layout_alignLeft="@+id/result_type6"
            android:layout_alignStart="@+id/result_type6"
            android:text="质量指数："/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_type8"
            android:layout_below="@+id/result_type7"
            android:layout_alignLeft="@+id/result_type7"
            android:layout_alignStart="@+id/result_type7"
            android:text="水分："/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_type9"
            android:layout_below="@+id/result_type8"
            android:layout_alignLeft="@+id/result_type8"
            android:layout_alignStart="@+id/result_type8"
            android:text="热量："/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_type10"
            android:layout_below="@+id/result_type9"
            android:layout_alignLeft="@+id/result_type9"
            android:layout_alignStart="@+id/result_type9"
            android:text="测量时间："/>

        <TextView
            android:layout_marginLeft="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="66kg"
            android:id="@+id/state_content_text_gy"
            android:layout_alignTop="@+id/state_content_text_gaoya"
            android:layout_toEndOf="@+id/state_content_text_gaoya"
            android:layout_toRightOf="@+id/state_content_text_gaoya"/>

        <TextView
            android:layout_marginLeft="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="32%"
            android:id="@+id/state_content_text_dy"
            android:layout_alignBaseline="@+id/state_content_text_diya"
            android:layout_alignBottom="@+id/state_content_text_diya"
            android:layout_toRightOf="@+id/state_content_text_diya"
            android:layout_toEndOf="@+id/state_content_text_diya" />

        <TextView
            android:layout_marginLeft="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="23kg"
            android:id="@+id/state_content_text_xl"
            android:layout_below="@+id/state_content_text_dy"
            android:layout_toRightOf="@+id/state_content_text_xinlv"
            android:layout_toEndOf="@+id/state_content_text_xinlv" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_data4"
            android:layout_below="@+id/state_content_text_xl"
            android:layout_toRightOf="@+id/result_taye4"
            android:layout_toEndOf="@+id/result_taye4"
            android:text="30kg"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_data5"
            android:layout_below="@+id/result_data4"
            android:layout_toRightOf="@+id/result_type5"
            android:layout_toEndOf="@+id/result_type5"
            android:text="13%"
            />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_data6"
            android:layout_below="@+id/result_data5"
            android:layout_toRightOf="@+id/result_type6"
            android:layout_toEndOf="@+id/result_type6"
            android:text="12"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_data7"
            android:layout_below="@+id/result_data6"
            android:layout_toRightOf="@+id/result_type7"
            android:layout_toEndOf="@+id/result_type7"
            android:text="35"
            />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_data8"
            android:layout_below="@+id/result_data7"
            android:layout_toRightOf="@+id/result_type8"
            android:layout_toEndOf="@+id/result_type8"
            android:text="45%"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_data9"
            android:layout_below="@+id/result_data8"
            android:layout_toRightOf="@+id/result_type9"
            android:layout_toEndOf="@+id/result_type9"
            android:text="3433"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/result_data10"
            android:layout_below="@+id/result_data9"
            android:layout_toRightOf="@+id/result_type10"
            android:layout_toEndOf="@+id/result_type10"
            android:text="2016-03-03 13:30"
            />






    </RelativeLayout>
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="150dp"
        android:background="#fff"
        android:id="@+id/state_content_relative_assessment_result"
        android:layout_below="@+id/state_content_relative_measure_result"
        android:layout_marginTop="20dp"
        android:layout_marginRight="20dp"
        android:layout_alignLeft="@+id/state_content_relative_measure_result"
        android:layout_alignStart="@+id/state_content_relative_measure_result">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="备注"
            android:textSize="20sp"
            android:id="@+id/state_content_text_assessment_result"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="17dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"/>

        <TextView
            android:text="人体成分详情测试数据"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/state_content_text_assessment__contentresult"
            android:layout_marginTop="8dp"
            android:layout_below="@+id/state_content_text_assessment_result"
            android:layout_alignStart="@+id/state_content_text_assessment_result"
            android:layout_alignLeft="@+id/state_content_text_assessment_result"/>
    </RelativeLayout>


</RelativeLayout>
